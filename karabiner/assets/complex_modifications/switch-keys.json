{
  "title": "switch control and capslock",
  "rules": [
    {
      "description": "switch control and capslock",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [ { "key_code": "right_control" } ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "left_control",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [ { "key_code": "caps_lock" } ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "switch command & option",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [ { "key_code": "left_option" } ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_option",
            "modifiers": { "optional": [ "any" ] }
          },
          "to": [ { "key_code": "left_command" } ]
        }
      ]
    }
  ]
}
