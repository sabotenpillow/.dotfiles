{
"title": "change left control and capslock",
    "rules": [
      {
        "description": "Caps lock to CTRL",
        "manipulators": [
          {
            "from": {
              "key_code": "caps_lock",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "right_control"
              }
            ],
            "type": "basic"
          }
        ]
      },
      {
        "description": "left CTRL to Caps lock",
        "manipulators": [
          {
            "from": {
              "key_code": "left_control",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "caps_lock"
              }
            ],
            "type": "basic"
          }
        ]
      }
    ]
}
