{
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.scrollBeyondLastLine": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.defaultInterpreterPath": "/Users/tone/.asdf/shims/python",
  "python.testing.pytestEnabled": true,
  "flake8.args": ["--max-line-length=88", "--extend-ignore=E203,W503"],
  "files.watcherExclude": {
    "**/homebrew/**": true
  },
  "vscode-neovim.neovimExecutablePaths.linux": "/home/tone/.asdf/shims/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/home/tone/.vim/nvim/config/init.vscode.vim",
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.vim/nvim/config/init.vscode.vim",
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    }
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "material-icon-theme",
  "security.workspace.trust.enabled": false,
  "workbench.colorTheme": "Winter is Coming (Dark Black)",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "python.analysis.typeCheckingMode": "standard",
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "geminicodeassist.project": "",
  "cloudcode.autoDependencies": "off"
}
